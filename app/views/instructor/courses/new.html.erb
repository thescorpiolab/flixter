<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new section</h1>
  <%= simple_form_for @course, url: instructor_courses_path, method: :post, multipart: :true do |f| %>
    <%= f.input :title %>
    <%= f.input :images %>
    <%= f.input :description %>
    <%= f.input :cost %>

    <br />
    <%= f.submit "Add a section", class: 'btn btn-primary' %>

    <br />
    <br />
  <% end %>
</div>
